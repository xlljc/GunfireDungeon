config_version=5

[application]

config/name="枪火地牢"
run/main_scene="res://scene/Main.tscn"
config/features=PackedStringArray("4.4", "C#")
config/icon="res://icon.png"

[autoload]

InitUiManager="*res://addons/ds_ui/InitUiManager.cs"

[debug]

settings/fps/force_fps=60

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="GunfireDungeon"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/ds_ui/plugin.cfg", "res://addons/game_plugin/plugin.cfg", "res://addons/node_presetting/plugin.cfg")

[file_customization]

folder_colors={
"res://prefab/": "orange",
"res://resource/": "blue",
"res://src/": "green"
}

[gui]

theme/custom="uid://ds668te2rph30"

